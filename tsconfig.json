{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@player": ["src/Player"],
      "@EnderWar/*": ["src/EnderWar/*"]
    },
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "module": "commonjs",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2019"
  },
  "include": ["src/**/*"]
}
